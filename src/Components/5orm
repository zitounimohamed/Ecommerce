                  <li class="nav-item">
                    <a href="test" class="icons">
                      <i class="ti-search" aria-hidden="true"></i>
                    </a>
                  </li>

                  <li class="nav-item">
                    <a href="/panier" class="icons">
                      <i class="ti-shopping-cart"></i>
                    </a>
                  </li>

                  { !this.props.isAuth ?
                 <li class="nav-item" key = "sign">
                    <a href="inscrit" class="icons">
                      <i class="ti-user" aria-hidden="true"></i>
                    </a>
                  </li>  : null }

                  { this.props.isAuth ?
                  <li class="nav-item">
                    <a href="signout" class="icons"  onClick={this.signOut}>
                      <i class="ti-power-off" aria-hidden="true" ></i>
                    </a>
                  </li>: null }

                  <li class="nav-item submenu dropdown">
                  <a href="/" class="nav-link dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true"
                      aria-expanded="false">
                        <a href="inscrit" class="icons">
                      <i class="ti-heart" aria-hidden="true"></i></a></a>
                      <ul class="dropdown-menu">
                      <li class="nav-item">
                        <a class="nav-link" href="/KIA">KIA</a>
                      </li>
                      </ul>
                      </li>
                     
                 
                </ul>
              </div>
            </div>
          </div>
        </nav>
      </div>
    </div></div>
        );
    }
}

function mapStateToProps(state) {
  return {
    isAuth: state.auth.isAuthenticated
  };
}

export default nav ;
//export default connect(mapStateToProps, actions)(nav);

