            import React, { Component,useState } from 'react';
import './admin.css';
import  Ajout from './ajoutpiece';
import Update from './gerercompte';
import bienvenue from './img/bienvenue.jpg';
import { TabContent, TabPane, Nav, NavItem, NavLink, Card, Button, CardTitle, CardText, Row, Col } from 'reactstrap';
import classnames from 'classnames';


const Admin = (props) => {
  const [activeTab, setActiveTab] = useState('1');

  const toggle = tab => {
    if(activeTab !== tab) setActiveTab(tab);
  }
 return (
   <div className="container">
     <img src={bienvenue} class="bienvenue" />
         <br/> <br/>
            <h2>Que voulez vous faire...?</h2>
         <br></br>
         <div>
      <Nav tabs>
        <NavItem>
          <NavLink
            className={classnames({ active: activeTab === '1' })}
            onClick={() => { toggle('1'); }}
          >
            Gérer les comptes 
          </NavLink>
        </NavItem>

        <NavItem>
          <NavLink
            className={classnames({ active: activeTab === '2' })}
            onClick={() => { toggle('2'); }}
          >
            Ajouter une pièce
          </NavLink>
        </NavItem>

        <NavItem>
          <NavLink
            className={classnames({ active: activeTab === '3' })}
            onClick={() => { toggle('3'); }}
          >
            Modifier une pièce
          </NavLink>
        </NavItem>

        <NavItem>
          <NavLink
            className={classnames({ active: activeTab === '4' })}
            onClick={() => { toggle('4'); }}
          >
            Supprimer une pièce
          </NavLink>
        </NavItem>

        <NavItem>
          <NavLink
            className={classnames({ active: activeTab === '5' })}
            onClick={() => { toggle('5'); }}
          >
            Afficher la liste des clients
          </NavLink>
        </NavItem>



      </Nav>
      <TabContent activeTab={activeTab}>
        <TabPane tabId="1">
          <Row>
            <Col sm="12">
              <h4>Tab 1 Contents</h4>
            </Col>
          </Row>
        </TabPane>
        <TabPane tabId="2">
   
              <Ajout />
             
        </TabPane>
        <TabPane tabId="3">
         
              <Update />
            
             
           
        </TabPane>
        <TabPane tabId="4">
          
        </TabPane>
        <TabPane tabId="5">
          <Row>
            <Col sm="6">
              
            </Col>
            <Col sm="6">
             
            </Col>
          </Row>
        </TabPane>
      </TabContent>
    </div>
   </div>
 );
}



export default Admin;
